<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Feed</title>

    <link rel="stylesheet" href="/style.css">

  </head>
  <body>

    <div id="container">

      <% include ../views/partials/header.ejs %>


    <% for (let photo of photos) { %>
          <li>
            <a href="/feed/<%= photo._id %>"><img src="<%= photo.url %>"></a><br>
            <%= photo.submitted_by %><br>
            <%= photo.caption %>
          </li>
      <% } %>



    </div>

  </body>
</html>
