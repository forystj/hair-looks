<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Feed</title>

    <link rel="stylesheet" href="/style.css">

  </head>
  <body>

    <div id="container">

      <% include ../views/partials/header.ejs %>

      <a href='/users/<%= foundId[0]._id %>'>View My Profile</a>


    <% for (let photo of photos) { %>
          <li>
            <a href="/feed/<%= photo._id %>"><img src="<%= photo.url %>"></a>
            <h4>Posted By: <%= photo.submitted_by %></h4>
          </li>
      <% } %>



    </div>

  </body>
</html>
